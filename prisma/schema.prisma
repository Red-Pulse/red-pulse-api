generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id   Int    @id @default(autoincrement())
  name String
  type String
}

enum BloodType {
  Positive_A
  Positive_B
  Positive_AB
  Positive_0
  Negative_A
  Negative_B
  Negative_AB
  Negative_0
}